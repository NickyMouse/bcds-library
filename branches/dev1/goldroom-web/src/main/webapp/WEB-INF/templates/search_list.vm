Total Result:$totalCount
#parse("navigator.vm")
<table width="100%" cellspacing="0" cellpadding="3" border="0">
	<tbody>
		<tr height="25">
			<td class="lines" width="15%" valign="top" bgcolor="#cce4a3" align="left">
				<strong>Image</strong>
			</td>
			<td class="lines" width="15%" valign="top" bgcolor="#cce4a3" align="left">
				<strong>Name</strong>
			</td>
			<td class="lines" width="10%" valign="top" bgcolor="#cce4a3" align="left">
				<strong>Edition</strong>
			</td>
			<td class="lines" width="137" valign="top" bgcolor="#cce4a3" align="left">
				<strong>Description</strong>
			</td>
			<td class="lines" width="109" valign="top" bgcolor="#cce4a3" align="left">
				<strong>Publish Time</strong>
			</td>
		</tr>
		#foreach($book in $list)
		<tr #if($velocityCount)class="highlight"#end height="120" >
			<td class="lines" width="35" valign="middle"  align="center" >
				<img class="bookImage" src="$book.bookImgUrl"/>
			</td>
			<td class="lines">$book.bookName</td>
			<td class="lines">$book.bookEdition</td>
			<td class="lines">
				<a href="http://www.opensubscriber.com/message/java-user@lucene.apache.org/7285227.html" title="">$book.bookDescription</a>
			</td>
			<td class="lines">$book.bookPublishTime</td>
		</tr>
		#end
</tbody>
</table>